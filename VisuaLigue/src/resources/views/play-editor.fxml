                        <fx:include source="graphics/3d-icon.fxml" />
                     </children>
                  </Pane>
               </graphic>
               <tooltip>
                  <Tooltip text="Vue 3D" />
               </tooltip>
            </Button>
            <Button mnemonicParsing="false" styleClass="borderless-button">
