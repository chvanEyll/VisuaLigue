                                          <HBox>
                                             <children>
                                                <Pane stylesheets="@../css/graphics/ball-object.fxml.css">
                                             <children>
                                                <fx:include source="graphics/ball-object.fxml" />
                                             </children>
                                          </Pane>
                                             </children>
                                             <styleClass>
