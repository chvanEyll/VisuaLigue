            <Button mnemonicParsing="false" styleClass="borderless-button">
               <graphic>
                  <Pane stylesheets="@../css/graphics/zoom-in-icon.fxml.css">
                     <children>
                        <fx:include source="graphics/zoom-in-icon.fxml" />
                     </children>
                  </Pane>
               </graphic>
               <tooltip>
                  <Tooltip text="Zoom avant" />
               </tooltip>
            </Button>
            <TextField prefWidth="85.0" text="100%">
