                                                      <Insets left="14.0" />
                                                   </padding>
                                                   <children>
                                                      <Pane stylesheets="@../css/graphics/player-object.fxml.css">
                                                         <children>
                                                            <fx:include source="graphics/player-object.fxml" />
                                                         </children>
                                                      </Pane>
                                                      <Label text="Gardien" />
                                                   </children>
                                                   <styleClass>
                                                      <String fx:value="player-item" />
                                                      <String fx:value="item" />
                                                   </styleClass>
                                                </HBox>
                                             </children>
                                          </VBox>
                                       </children>
                                    </VBox>
                                 </children>
                              </VBox>
                              <VBox>
                                 <children>
                                    <HBox alignment="CENTER_LEFT" fillHeight="false" prefHeight="50.0" spacing="14.0" styleClass="section-header">
                                       <padding>
                                          <Insets left="14.0" />
                                       </padding>
                                       <children>
                                          <Pane stylesheets="@../css/graphics/object-icon.fxml.css">
                                             <children>
                                                <fx:include source="graphics/object-icon.fxml" />
                                             </children>
                                          </Pane>
                                          <Label text="Objets" />
                                       </children>
                                    </HBox>
                                    <TilePane prefColumns="3" prefRows="3" prefTileHeight="50.0" prefTileWidth="50.0">
