                  <HBox alignment="CENTER_RIGHT" prefHeight="50.0" styleClass="toggle-container" VBox.vgrow="ALWAYS">
                     <children>
                        <VBox onMouseClicked="#onHamburgerIconClick" prefWidth="30.0" spacing="6.0" styleClass="toggle">
                           <children>
                              <Line endX="30.0" stroke="GREY" styleClass="line" />
                              <Line endX="30.0" stroke="GREY" styleClass="line" />
                              <Line endX="30.0" stroke="GREY" styleClass="line" />
                           </children>
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                           <padding>
                              <Insets left="14.0" right="14.0" top="17.0" />
                           </padding>
                        </VBox>
                     </children>
                  </HBox>
                  <ScrollPane fitToWidth="true" hbarPolicy="NEVER" pannable="true" style="-fx-background: transparent;" styleClass="edge-to-edge" VBox.vgrow="ALWAYS">
                     <content>
                        <VBox>
                           <children>
                              <VBox>
                                 <children>
                                    <HBox alignment="CENTER_LEFT" fillHeight="false" prefHeight="50.0" spacing="14.0" styleClass="section-header">
                                       <padding>
                                          <Insets left="14.0" />
                                       </padding>
                                       <children>
                                          <Pane stylesheets="@../css/graphics/player-object-icon.fxml.css">
                                             <children>
                                                <fx:include source="graphics/player-object-icon.fxml" />
                                             </children>
                                          </Pane>
                                          <Label text="Joueurs" />
                                       </children>
                                       <VBox.margin>
                                          <Insets />
                                       </VBox.margin>
                                    </HBox>
                                    <VBox>
                                       <children>
                                          <HBox alignment="CENTER_LEFT" prefHeight="30.0" styleClass="player-type-section">
                                             <children>
                                                <Label text="Alliés" />
                                             </children>
                                             <padding>
                                                <Insets left="14.0" />
                                             </padding>
                                          </HBox>
                                          <VBox>
                                             <children>
                                                <HBox alignment="CENTER_LEFT" fillHeight="false" prefHeight="50.0" spacing="14.0">
                                                   <padding>
                                                      <Insets left="14.0" />
                                                   </padding>
                                                   <children>
                                                      <Pane stylesheets="@../css/graphics/player-object.fxml.css">
                                                         <children>
                                                            <fx:include source="graphics/player-object.fxml" />
                                                         </children>
                                                      </Pane>
                                                      <Label text="Attaquant" />
                                                   </children>
                                                   <styleClass>
                                                      <String fx:value="player-item" />
                                                      <String fx:value="item" />
                                                   </styleClass>
                                                </HBox>
                                                <HBox alignment="CENTER_LEFT" fillHeight="false" layoutX="10.0" layoutY="10.0" prefHeight="50.0" spacing="14.0">
                                                   <padding>
                                                      <Insets left="14.0" />
                                                   </padding>
                                                   <children>
                                                      <Pane stylesheets="@../css/graphics/player-object.fxml.css">
                                                         <children>
                                                            <fx:include source="graphics/player-object.fxml" />
                                                         </children>
                                                      </Pane>
                                                      <Label text="Gardien" />
                                                   </children>
                                                   <styleClass>
                                                      <String fx:value="player-item" />
                                                      <String fx:value="item" />
                                                   </styleClass>
                                                </HBox>
                                             </children>
                                          </VBox>
                                       </children>
                                    </VBox>
                                    <VBox layoutX="10.0" layoutY="60.0">
                                       <children>
                                          <HBox alignment="CENTER_LEFT" prefHeight="30.0" styleClass="player-type-section">
                                             <children>
                                                <Label text="Adverses" />
                                             </children>
                                             <padding>
                                                <Insets left="14.0" />
                                             </padding>
                                          </HBox>
                                          <VBox>
                                             <children>
                                                <HBox alignment="CENTER_LEFT" fillHeight="false" prefHeight="50.0" spacing="14.0">
                                                   <padding>
                                                      <Insets left="14.0" />
                                                   </padding>
                                                   <children>
                                                      <Pane stylesheets="@../css/graphics/player-object.fxml.css">
                                                         <children>
                                                            <fx:include source="graphics/player-object.fxml" />
                                                         </children>
                                                      </Pane>
                                                      <Label text="Attaquant" />
                                                   </children>
                                                   <styleClass>
                                                      <String fx:value="player-item" />
                                                      <String fx:value="item" />
                                                   </styleClass>
                                                </HBox>
                                                <HBox alignment="CENTER_LEFT" fillHeight="false" layoutX="10.0" layoutY="10.0" prefHeight="50.0" spacing="14.0">
