<BorderPane HBox.hgrow="ALWAYS" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
   <stylesheets>
      <URL value="@../css/main.fxml.css" />
      <URL value="@../css/play-editor.fxml.css" />
   </stylesheets>
   <styleClass>
      <String fx:value="root-section-pane" />
      <String fx:value="edge-to-edge" />
   </styleClass>
   <top>
      <HBox alignment="CENTER_LEFT" prefHeight="50.0" styleClass="toolbar">
         <children>
            <Button mnemonicParsing="false" styleClass="borderless-button">
               <graphic>
                   <Pane stylesheets="@../css/graphics/save-icon.fxml.css">
                     <children>
                        <fx:include source="graphics/save-icon.fxml" />
                     </children>
                  </Pane>
               </graphic>
               <tooltip>
                  <Tooltip text="Sauvegarder" />
               </tooltip>
            </Button>
            <Button mnemonicParsing="false" styleClass="borderless-button">
               <graphic>
                  <Pane stylesheets="@../css/graphics/cross-icon.fxml.css">
                     <children>
                        <fx:include source="graphics/cross-icon.fxml" />
                     </children>
                  </Pane>
               </graphic>
               <tooltip>
                  <Tooltip text="Fermer" />
               </tooltip>
            </Button>
            <Button mnemonicParsing="false" styleClass="borderless-button">
               <graphic>
                  <Pane stylesheets="@../css/graphics/export-icon.fxml.css">
