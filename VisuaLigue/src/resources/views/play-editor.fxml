            <Button mnemonicParsing="false" styleClass="borderless-button">
               <graphic>
                  <Pane stylesheets="@../css/graphics/zoom-out-icon.fxml.css">
                     <children>
                        <fx:include source="graphics/zoom-out-icon.fxml" />
