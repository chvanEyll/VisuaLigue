<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<ScrollPane fitToWidth="true" hbarPolicy="NEVER" HBox.hgrow="ALWAYS" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
   <content>
      <VBox spacing="14.0">
         <padding>
            <Insets bottom="14.0" left="14.0" right="14.0" top="14.0" />
         </padding>
         <children>
            <fx:include source="breadcrumb-nav-bar.fxml" />
            <VBox spacing="7.0" styleClass="form-group">
               <children>
                  <Label text="Nom du sport" />
                  <TextField />
               </children>
            </VBox>
            <AnchorPane>
               <children>
                  <HBox spacing="10.0" styleClass="button-group" AnchorPane.rightAnchor="0.0">
                     <children>
                        <Button defaultButton="true" mnemonicParsing="false" text="Continuer" textFill="#1895fd">
                           <styleClass>
                              <String fx:value="default" />
                              <String fx:value="standard-button" />
                           </styleClass></Button>
                        <Button cancelButton="true" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" styleClass="standard-button" text="Annuler" textFill="#1895fd" />
                     </children>
                  </HBox>
               </children>
            </AnchorPane>
         </children>
      </VBox>
   </content>
   <stylesheets>
      <URL value="@../css/main.fxml.css" />
   </stylesheets>
   <styleClass>
      <String fx:value="root-section-pane" />
      <String fx:value="edge-to-edge" />
   </styleClass>
</ScrollPane>
